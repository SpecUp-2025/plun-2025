<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<mapper namespace="com.spec.plun.auth.DAO.QualificationDAO">
  <select id="getByEmail"
          parameterType="string"
          resultType="com.spec.plun.auth.DTO.QualificationCheckDTO">
	SELECT 
		email,
		password
	FROM TB_MEMBER
	WHERE email = #{email} AND delete_yn='N' 

</select>
</mapper>	
