<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spec.plun.member.DAO.MemberDAO">
<select id="getEmail" parameterType="string" resultType="com.spec.plun.member.DTO.MemberDTO">
	SELECT
		email,
		name
	FROM TB_MEMBER
	WHERE email = #{email}

</select>


</mapper>